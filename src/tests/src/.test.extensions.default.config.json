{
	"structure": {
		"customTypes": "customType",
		"components": {
			"type": "component",
			"extensions": "component"
		},
		"utils": "util",
		"hooks": {
			"type": "hook",
			"extensions": {
				"index": "hookIndex",
				"default": "hookDefault"
			},
			"generateIndex": true
		}
	},
	"defaultStructureItem": {
		"extensions": "defaultStructureItem",
		"generateIndex": true
	},
	"blueprints": ["src/tests/src/blueprints"],
	"language": "react-typescript",
	"plugins": ["@amphore-dev/architect-plugin-react"]
}
